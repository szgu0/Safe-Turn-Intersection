    PCClip: {fileID: 8300000, guid: 128e28150be9770478afe1e9a7ae428a, type: 3}
    VRText: "\u6B61\u8FCE\u4F86\u5230\u300C\u5B89\u5168\u904E\u5F4E\u7684\u884C\u8ECA\u6280\u5DE7\u300D\u9019\u88E1\u662F
      VR \u65B0\u624B\u6559\u5B78\u7A7A\u9593\uFF0C\u5728\u9019\u88E1\u53EF\u4EE5\u5B78\u7FD2\u672C\u6559\u6750\u6240\u9700\u8981\u7684\u64CD\u4F5C\u65B9\u5F0F\u3002"
